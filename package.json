{
	"private": true,
	"name": "dominion-strategy-wiki",
	"version": "0.0.0",
	"packageManager": "pnpm@10.14.0",
	"scripts": {
		"build": "tsc && node dist/scripts/build-common.js",
		"type-check": "tsc -p tsconfig.common.json --noEmit && tsc --noEmit",
		"tidy": "biome check --write . && prettier --write .",
		"tidy:check": "biome check . && prettier --check .",
		"deploy-mediawiki": "node --enable-source-maps dist/scripts/deploy-to-mediawiki.js",
		"dev": "nodemon --watch src/dev --ignore src/dev/ui/dist --ext ts,tsx,css,html --exec \"cd src/dev/api && tsx server.ts\"",
		"test": "vitest",
		"test:run": "vitest run"
	},
	"dependencies": {
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.1",
		"axios": "^1.11.0",
		"cheerio": "^1.0.0-rc.12",
		"cors": "^2.8.5",
		"express": "^4.18.2",
		"glob": "^11.0.3",
		"pino": "^9.8.0",
		"pino-pretty": "^13.1.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"ws": "^8.18.3"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.1.4",
		"@types/jquery": "^3.5.29",
		"@types/node": "^24.2.1",
		"@types/react": "^18.2.66",
		"@types/react-dom": "^18.2.22",
		"@types/ws": "^8.18.1",
		"@vitejs/plugin-react": "^4.2.1",
		"dotenv": "^17.2.1",
		"esbuild": "^0.25.9",
		"nodemon": "^3.1.10",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"tsx": "^4.20.4",
		"typescript": "^5.3.0",
		"vite": "^5.2.0",
		"vitest": "^3.0.0"
	}
}
